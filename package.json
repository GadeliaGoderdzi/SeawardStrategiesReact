{
  "name": "fullstack-web-app",
  "version": "1.0.0",
  "description": "A comprehensive full stack web application with React frontend and Node.js backend",
  "main": "server/src/app.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "start": "node server/src/app.js",
    "server:dev": "cd server && npm run dev",
    "server:start": "cd server && npm start",
    "client:dev": "cd client && npm start",
    "client:build": "cd client && npm run build",
    "build": "npm run client:build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .js,.jsx",
    "lint:fix": "eslint . --ext .js,.jsx --fix",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check .",
    "install:all": "npm install && cd client && npm install && cd ../server && npm install",
    "clean": "rimraf node_modules client/node_modules server/node_modules client/build server/build coverage",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "heroku-postbuild": "npm run client:build"
  },
  "keywords": [
    "fullstack",
    "react",
    "nodejs",
    "express",
    "mongodb",
    "javascript",
    "web-application"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/fullstack-web-app.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/fullstack-web-app/issues"
  },
  "homepage": "https://github.com/yourusername/fullstack-web-app#readme",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.0",
    "@babel/eslint-parser": "^7.22.0",
    "@babel/plugin-proposal-class-properties": "^7.18.0",
    "@babel/plugin-proposal-decorators": "^7.22.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.0",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/plugin-transform-runtime": "^7.22.0",
    "@babel/preset-env": "^7.22.0",
    "@babel/preset-react": "^7.22.0",
    "@testing-library/jest-dom": "^5.16.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.0",
    "babel-jest": "^29.5.0",
    "concurrently": "^8.2.0",
    "css-minimizer-webpack-plugin": "^5.0.0",
    "eslint": "^8.42.0",
    "eslint-plugin-import": "^2.27.0",
    "eslint-plugin-jsx-a11y": "^6.7.0",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-transform-css": "^6.0.0",
    "jest-transform-file": "^1.1.0",
    "mini-css-extract-plugin": "^2.7.0",
    "postcss": "^8.4.0",
    "postcss-loader": "^7.3.0",
    "prettier": "^2.8.0",
    "rimraf": "^5.0.0",
    "sass": "^1.62.0",
    "sass-loader": "^13.3.0",
    "terser-webpack-plugin": "^5.3.0",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.0",
    "webpack-dev-server": "^4.15.0"
  },
  "dependencies": {
    "core-js": "^3.31.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}